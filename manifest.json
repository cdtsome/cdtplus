	"content_scripts": [
		{
			"matches": [
				"https://ke.codingtang.com/",
				"https://newcourse.codingtang.com/*"
			],
			"js": ["jquery.min.js", "ke.js"]
		}, 
		{
			"matches": ["https://wenda.codingtang.com/*"],
			"js": ["jquery.min.js", "wenda.js"]
		}, 
		{
			"matches": ["https://kuding1024.com/*"],
			"js": ["jquery.min.js", "dev.js"]
		}
	]
}